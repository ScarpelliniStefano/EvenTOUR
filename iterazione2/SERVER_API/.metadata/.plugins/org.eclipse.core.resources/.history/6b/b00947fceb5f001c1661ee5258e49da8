package com.scarcolo.eventour.model;


// TODO: Auto-generated Javadoc
/**
 * The Class Location.
 */
public class Location {
	
	/** The locality. */
	private String locality;
	
	/** The city. */
	private String city;
	
	/** The cap. */
	private String cap;
	
	/** The provincia. */
	private String provincia;
	
	/** The sigla. */
	private String sigla;
	
	/** The regione. */
	private String regione;
	
	/** The lat. */
	private Double lat;
	
	/** The lng. */
	private Double lng;
	
	/**
	 * Gets the locality.
	 *
	 * @return the locality
	 */
	public String getLocality() {
		return locality;
	}
	
	/**
	 * Sets the locality.
	 *
	 * @param locality the new locality
	 */
	public void setLocality(String locality) {
		this.locality = locality;
	}
	
	/**
	 * Gets the city.
	 *
	 * @return the city
	 */
	public String getCity() {
		return city;
	}
	
	/**
	 * Sets the city.
	 *
	 * @param city the new city
	 */
	public void setCity(String city) {
		this.city = city;
	}
	
	/**
	 * Gets the cap.
	 *
	 * @return the cap
	 */
	public String getCap() {
		return cap;
	}
	
	/**
	 * Sets the cap.
	 *
	 * @param cap the new cap
	 */
	public void setCap(String cap) {
		this.cap = cap;
	}
	
	/**
	 * Gets the provincia.
	 *
	 * @return the provincia
	 */
	public String getProvincia() {
		return provincia;
	}
	
	/**
	 * Sets the provincia.
	 *
	 * @param provincia the new provincia
	 */
	public void setProvincia(String provincia) {
		this.provincia = provincia;
	}
	
	/**
	 * Gets the sigla.
	 *
	 * @return the sigla
	 */
	public String getSigla() {
		return sigla;
	}
	
	/**
	 * Sets the sigla.
	 *
	 * @param sigla the new sigla
	 */
	public void setSigla(String sigla) {
		this.sigla = sigla;
	}
	
	/**
	 * Gets the regione.
	 *
	 * @return the regione
	 */
	public String getRegione() {
		return regione;
	}
	
	/**
	 * Sets the regione.
	 *
	 * @param regione the new regione
	 */
	public void setRegione(String regione) {
		this.regione = regione;
	}
	
	/**
	 * Gets the lat.
	 *
	 * @return the lat
	 */
	public Double getLat() {
		return Double.lat;
	}
	
	/**
	 * Sets the lat.
	 *
	 * @param lat the new lat
	 */
	public void setLat(Float lat) {
		this.lat = lat;
	}
	
	/**
	 * Gets the lng.
	 *
	 * @return the lng
	 */
	public Float getLng() {
		return lng;
	}
	
	/**
	 * Sets the lng.
	 *
	 * @param lng the new lng
	 */
	public void setLng(Float lng) {
		this.lng = lng;
	}
}
